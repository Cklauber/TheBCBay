@model IEnumerable<TheBCBay.Models.ItemModel>


<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>This is our sample</title>
</head>
<body>
    <table>
        @foreach (var ItemModel in Model)
        {
            <tr>
                <td>@ItemModel.Id - </td>
                <td> @ItemModel.Title</td>
            </tr>

        }
    </table>
</body>
</html>